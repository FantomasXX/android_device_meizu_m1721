# Goodix Fingerprint
/dev/ttyACM[0-9]*	u:object_r:goodixfingerprintd_device:s0
/dev/goodix_fp*	u:object_r:goodixfingerprintd_device:s0
/data/fingerprint(/.*)?	u:object_r:goodixfingerprintd_data_file:s0
/data/fingerprint/gxfp(/.*)?	u:object_r:goodixfingerprintd_data_file:s0
/data/system/fingerprint(/.*)?	u:object_r:goodixfingerprintd_data_file:s0
/dev/gf66xx-spi	u:object_r:goodixfingerprintd_device:s0
/system/bin/goodixfingerprintd	u:object_r:goodixfingerprintd_exec:s0

# Glove Mode
/sys/class/tp_glove/tp_glove/glove_enable	u:object_r:proc_touchpanel:s0

# Block devices
/dev/block/bootdevice/by-name/persist		u:object_r:persist_block_device:s0

# Data files
/data/misc/netmgr/log\.txt			        u:object_r:netmgrd_data_file:s0

# aw2013
/sys/devices/soc/7af6000.i2c/i2c-6/6-0045/leds/mx-led/blink	u:object_r:sysfs_leds:s0
/sys/devices/soc/7af6000.i2c/i2c-6/6-0045/leds/mx-led/brightness	u:object_r:sysfs_leds:s0
